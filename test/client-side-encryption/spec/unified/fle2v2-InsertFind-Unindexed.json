  "tests": [
    {
      "description": "Insert and find FLE2 unindexed field",
      "operations": [
        {
          "name": "insertOne",
          "arguments": {
            "document": {
              "_id": 1,
              "encryptedUnindexed": "value123"
            }
          },
          "object": "coll"
        },
        {
          "name": "find",
          "arguments": {
            "filter": {
              "_id": 1
            }
          },
          "object": "coll",
          "expectResult": [
            {
              "_id": 1,
              "encryptedUnindexed": "value123"
            }
          ]
        },
        {
          "name": "find",
          "object": "coll_unencrypted",
          "arguments": {
            "filter": {}
          },
          "expectResult": [
            {
              "_id": 1,
              "encryptedUnindexed": {
                "$$type": "binData"
              }
            }
          ]
        }
      ],
