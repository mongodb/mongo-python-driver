                        }
                      }
                    ]
                  },
                  "$db": "keyvault",
                  "readConcern": {
                    "level": "majority"
                  }
                },
                "commandName": "find"
              }
            },
            {
              "commandStartedEvent": {
                "command": {
                  "insert": "default",
                  "documents": [
                    {
                      "_id": 1,
                      "encryptedUnindexed": {
                        "$$type": "binData"
                      }
                    }
                  ],
                  "ordered": true
                },
                "commandName": "insert"
              }
            },
            {
              "commandStartedEvent": {
                "command": {
                  "find": "default",
                  "filter": {
                    "_id": {
                      "$eq": 1
                    }
                  }
                },
                "commandName": "find"
