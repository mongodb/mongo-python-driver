  "tests": [
    {
      "description": "client bulkWrite QE replaceOne",
      "operations": [
        {
          "object": "collection0",
          "name": "insertMany",
          "arguments": {
            "documents": [
              {
                "_id": 1,
                "encryptedInt": 11
              },
              {
                "_id": 2,
                "encryptedInt": 22
              },
              {
                "_id": 3,
                "encryptedInt": 33
              }
            ]
          }
        },
        {
          "object": "client0",
          "name": "clientBulkWrite",
          "arguments": {
            "models": [
              {
                "replaceOne": {
                  "namespace": "crud-tests.coll0",
                  "filter": {
                    "encryptedInt": {
                      "$eq": 11
                    }
                  },
                  "replacement": {
                    "encryptedInt": 44
                  }
                }
              }
            ]
          },
          "expectResult": {
            "insertedCount": 0,
            "upsertedCount": 0,
            "matchedCount": 1,
            "modifiedCount": 1,
            "deletedCount": 0
          }
        },
        {
          "object": "collection0",
          "name": "find",
          "arguments": {
            "filter": {
              "encryptedInt": 44
            }
          },
          "expectResult": [
            {
              "_id": 1,
              "encryptedInt": 44
            }
          ]
        },
        {
          "object": "collection2",
          "name": "find",
          "arguments": {
            "filter": {}
          },
          "expectResult": [
            {
              "_id": 1,
              "encryptedInt": {
                "$$type": "binData"
              },
              "__safeContent__": {
                "$$type": "array"
              }
            },
            {
              "_id": 2,
              "encryptedInt": {
                "$$type": "binData"
              },
              "__safeContent__": {
                "$$type": "array"
              }
            },
            {
