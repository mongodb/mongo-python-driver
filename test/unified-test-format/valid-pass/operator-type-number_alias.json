      "collectionName": "coll0",
      "databaseName": "test",
      "documents": []
    }
  ],
  "tests": [
    {
      "description": "type number alias matches int32",
      "operations": [
        {
          "name": "insertOne",
          "object": "collection0",
          "arguments": {
            "document": {
              "_id": 1,
              "x": {
                "$numberInt": "2147483647"
              }
            }
          }
        },
        {
          "name": "find",
          "object": "collection0",
          "arguments": {
            "filter": {
              "_id": 1
            },
            "limit": 1
          },
          "expectResult": [
            {
              "_id": 1,
              "x": {
                "$$type": "number"
              }
            }
          ]
        }
      ]
    },
